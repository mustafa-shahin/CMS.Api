{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=cms_db;Username=cms_user;Password=cms_password"
  },
  "Jwt": {
    "Key": "DevelopmentSecretKeyThatIsAtLeast32CharactersLong!DoNotUseInProduction!",
    "Issuer": "CMS.Api.Development",
    "Audience": "CMS.Client.Development",
    "AccessTokenExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 30
  },
  "RateLimiting": {
    "EnableRateLimiting": true,
    "PermitLimit": 1000,
    "Window": 60
  }
}